// Implementation of the example described in chapter 2
// of the Jason's manual

MAS mars {

    environment: MarsEnvStd(90, 1) 
        // parameters: 1. sleep time (in ms) after each action - MUST be multiple of 5 ()
        //             2. number of 'K' percepts (0 / 10 / 100 / 200 / 400 / 800)

    agents:
        agent 
            [beliefs="whatever"];

    aslSourcePath:
        "src/asl";
}
